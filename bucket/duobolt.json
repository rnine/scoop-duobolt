{
    "version": "0.3.82",
    "description": "Fast duplicate file finder using BLAKE3 hashing",
    "homepage": "https://duobolt.app",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://downloads.duobolt.app/windows/cli/x86_64/duobolt-cli-0.3.82.zip",
            "hash": "Not"
        },
        "arm64": {
            "url": "https://downloads.duobolt.app/windows/cli/aarch64/duobolt-cli-0.3.82.zip",
            "hash": "Not"
        }
    },
    "bin": "duobolt-cli.exe",
    "checkver": {
        "url": "https://downloads.duobolt.app/version.txt",
        "regex": "([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.duobolt.app/windows/cli/x86_64/duobolt-cli-$version.zip"
            },
            "arm64": {
                "url": "https://downloads.duobolt.app/windows/cli/aarch64/duobolt-cli-$version.zip"
            }
        }
    },
    "notes": [
        "Documentation: https://duobolt.app/docs/",
        "",
        "Usage:",
        "  duobolt-cli scan C:\\path\\to\\folder",
        "  duobolt-cli --help"
    ]
}
